.fa-flip-vertical { transform: scale(1, -1); }

.fa-flip-both, .fa-flip-horizontal.fa-flip-vertical { transform: scale(-1, -1); }

.fa-rotate-by { transform: rotate(var(--fa-rotate-angle,none)); }

